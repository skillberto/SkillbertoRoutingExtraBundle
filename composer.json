{
    "name":         "skillberto/routing-extra-bundle",
    "description":  "This bundle is an extra router. With SonataAdminBundle can routing from database, and create custom database routing",
    "keywords":     ["menu","database-menu","admin-menu"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":      [
        {
            "name":     "Heiszler Norbert",
            "email":    "skillbertoo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": ">=2.1",
        "sonata-project/block-bundle": "2.2.*@dev",
        "sonata-project/cache-bundle": "2.1.*@dev",
        "sonata-project/admin-bundle": "2.2.*@dev"
    },
    "suggest": {
        "skillberto/admin-bundle": "dev-master"
    },
    "autoload":     {
        "psr-0": { "Skillberto\\RoutingExtraBundle": "" }
    },
    "target-dir": "Skillberto/RoutingExtraBundle"
}
